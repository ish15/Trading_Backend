    "concurrently": "^8.2.2",
    "config": "^3.2.2",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.5.0",
    "mongoose": "^5.6.8",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "react-tabulator": "^0.9.1"
  },
